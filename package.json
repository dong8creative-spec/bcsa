{
  "name": "busan-ycc-website",
  "version": "1.0.0",
  "description": "부산청년사업가들 웹사이트",
  "type": "module",
  "scripts": {
    "build:css": "tailwindcss -i ./src/input.css -o ./assets/css/tailwind.css --minify",
    "build:js": "babel index.html --out-file index-compiled.html --presets=@babel/preset-react",
    "build": "npm run build:css && npm run build:js",
    "watch:css": "tailwindcss -i ./src/input.css -o ./assets/css/tailwind.css --watch",
    "dev": "npm run watch:css",
    "server": "node server.js",
    "http": "node http-server.js",
    "dev:all": "concurrently \"npm run server\" \"npm run dev\"",
    "start": "concurrently \"npm run http\" \"npm run server\" \"npm run dev\"",
    "deploy": "firebase deploy",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:all": "firebase deploy",
    "emulators": "firebase emulators:start",
    "emulators:functions": "firebase emulators:start --only functions"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "tailwindcss": "^3.3.6",
    "concurrently": "^8.2.2"
  }
}

